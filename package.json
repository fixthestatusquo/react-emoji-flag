{
  "name": "react-emoji-flag",
  "version": "1.1.4",
  "type": "module",
  "source": "src/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "publishConfig": {
    "source": "src/index.js",
    "main": "dist/index.js"
  },
  "main": "src/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "microbundle --jsx React.createElement --jsxFragment React.Fragment --no-compress",
    "build:bundle": "microbundle",
    "lint": "npx eslint src/index.js",
    "dev": "microbundle watch --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react --globals react/jsx-runtime=jsx"
  },
  "license": "MIT",
  "keywords": [
    "emoji",
    "react",
    "country",
    "country-code",
    "flag",
    "iso-3166",
    "iso-3166-1_alpha-2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fixthestatusquo/react-emoji-flag.git"
  },
  "bugs": {
    "url": "https://github.com/fixthestatusquo/react-emoji-flag/issues"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "microbundle": "^0.15.1"
  }
}
